FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      c;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wall
    {
        type            zeroGradient;
    }

    side
    {
        type            zeroGradient;
    }


    topmid
    {
        type            fixedValue;
        value           uniform 1;
    }

    bottom
    {
        type            dynamicRetention;
        value uniform 0;
        S uniform 0;
        RobinD   uniform 1e-5;
        RobinK   uniform -1e-5;
        forwardReaction
        {
          type constant;
        }
        backwardReaction
        {
          type constant;
        }
        Kd uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
