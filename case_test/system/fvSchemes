FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
    binaryReaction  Euler;
}

gradSchemes
{
    default         pointCellsLeastSquares;
}

divSchemes
{
    default         Gauss linear;
    div(phi,U)      Gauss upwind; //bounded Gauss linearUpwind grad(U);
    div((nu*dev2(grad(U).T()))) Gauss linear;
    div((nuEff*dev2(T(grad(U))))) Gauss linear;
    div(phi,psi)      Gauss upwind; //bounded Gauss vanLeer;
    div(phi,xi)      Gauss upwind; //bounded Gauss vanLeer;
    div(phi,chi)      Gauss upwind; //bounded Gauss vanLeer;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}


// ************************************************************************* //
